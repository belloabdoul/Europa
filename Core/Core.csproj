<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <TargetFramework>net9.0</TargetFramework>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    </PropertyGroup>

    <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
      <IsAotCompatible>False</IsAotCompatible>
    </PropertyGroup>

    <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
      <IsAotCompatible>False</IsAotCompatible>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Blake3" Version="1.1.0" />
        <PackageReference Include="CommunityToolkit.HighPerformance" Version="8.3.2" />
        <PackageReference Include="DotNext.Unsafe" Version="5.13.0" />
        <PackageReference Include="FluentValidation" Version="11.10.0" />
        <PackageReference Include="Microsoft.EntityFrameworkCore" Version="9.0.0-rc.2.24474.1" />
        <PackageReference Include="Microsoft.Extensions.Hosting" Version="9.0.0-rc.2.24473.5" />
        <PackageReference Include="NRedisStack" Version="0.13.0" />
        <PackageReference Include="Sdcb.LibRaw" Version="0.21.1.6" />
        <PackageReference Include="Sdcb.LibRaw.runtime.linux64" Version="0.21.1" />
        <PackageReference Include="Sdcb.LibRaw.runtime.win32" Version="0.21.1" />
        <PackageReference Include="Sdcb.LibRaw.runtime.win64" Version="0.21.1" />
        <PackageReference Include="SoundFingerprinting" Version="11.0.0" />
        <PackageReference Include="U8String" Version="0.16.0-alpha" />
        <PackageReference Include="U8String.Optimization" Version="0.16.0-alpha">
          <PrivateAssets>all</PrivateAssets>
          <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        </PackageReference>
    </ItemGroup>

    <ItemGroup Condition="'$(TargetFramework)' == 'net9.0'">
      <PackageReference Include="DotNext" Version="5.13.0" />
    </ItemGroup>

</Project>
